{
  "schemaVersion": 1,
  "id": "auto-map-art",
  "version": "${version}",
  "name": "Auto Map Art",
  "description": "Auto builds any flat map art or similar structure",
  "authors": [
    "WildBush"
  ],
  "contact": {
    "repo": "https://github.com/underscoreL0K1/Bell-Hack"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.AutoMapArt.addon.Addon"
    ]
  },
  "mixins": [
    "addon-template.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "255,230,0"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}